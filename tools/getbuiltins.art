#!/usr/bin/env arturo
print "IN GETBUILTINS"
builtins1: []
builtins2: []

loop symbols [k,v][
    switch suffix? k "?" -> 'builtins1 ++ k
                         -> 'builtins2 ++ k
]
print "-here1"
map 'builtins1 => chop
sort 'builtins1
sort 'builtins2
print "-here2"
if arg\0 = "1" [
    print "ARG 0 = 1"
    prints [join.with:"|" builtins1]
]
if arg\0 = "2" [
    print "ARG 0 = 2"
    prints [join.with:"|" builtins2]
]

;print ["total:" (size builtins1) + (size builtins2)]