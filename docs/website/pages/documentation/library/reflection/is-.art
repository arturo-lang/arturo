name: "is?"
type: :function
module: "Reflection"
description: "check whether value is of given type"
returns: [
        :logical
    ]
examples: [
        {
            is? :string "hello"       ; => true
            is? :block [1 2 3]        ; => true
            is? :integer "boom"       ; => false
            
            is? [:string] ["one" "two"]     ; => true
            is? [:integer] [1 "two]         ; => false
        }
    ]
attributes: #[
    ]
arguments: #[
    type: [
            :type
             :block
        ]
    value: [
            :any
        ]
    ]
related: [
        "Reflection/integer?"
         "Reflection/set?"
         "Reflection/literal?"
         "Reflection/string?"
    ]