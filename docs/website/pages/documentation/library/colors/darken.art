name: "darken"
type: :function
module: "Colors"
description: "darken color by given percentage (0.0-1.0)"
returns: [
        :color
    ]
examples: [
        {
            darken #red 0.2         ; => #CC0000
            darken #red 0.5         ; => #7F0000
            
            darken #9944CC 0.3      ; => #6B308F
        }
    ]
attributes: #[
    ]
arguments: #[
    color: [
            :color
        ]
    percent: [
            :floating
        ]
    ]
related: [
        "Colors/saturate"
         "Colors/lighten"
         "Colors/desaturate"
         "Colors/blend"
    ]