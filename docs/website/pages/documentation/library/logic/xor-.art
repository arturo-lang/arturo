name: "xor?"
type: :function
module: "Logic"
description: "return the logical XOR for the given values"
returns: [
        :logical
    ]
examples: [
        {
            x: 2
            y: 3
            
            if? xor? x=2 y=3 [
                print "yep, that's correct!"]
            ]
            else [
                print "nope, that's not correct"
            ]
            
            ; nope, that's not correct
        }
    ]
attributes: #[
    ]
arguments: #[
    valueA: [
            :logical
             :block
        ]
    valueB: [
            :logical
             :block
        ]
    ]
related: [
        "Logic/nor?"
         "Logic/or?"
         "Logic/xnor?"
         "Binary/xor"
    ]