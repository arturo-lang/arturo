name: "conj"
type: :function
module: "Numbers"
description: "calculate the complex conjugate of given number"
returns: [
        :complex
    ]
examples: [
        {
            b: to :complex [1 2]        ; b: 1.0+2.0i
            print conj b                ; 1.0-2.0i
        }
    ]
attributes: #[
    ]
arguments: #[
    number: [
            :complex
        ]
    ]
related: [
        "Files/copy"
         "Numbers/cos"
         "Numbers/cosh"
         "Arithmetic/inc"
    ]