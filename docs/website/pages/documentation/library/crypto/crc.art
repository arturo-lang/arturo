name: "crc"
type: :function
module: "Crypto"
description: "calculate the CRC32 polynomial of given string"
returns: [
        :string
         :nothing
    ]
examples: [
        {
            print crc "The quick brown fox jumps over the lazy dog"
            ; 414FA339
        }
    ]
attributes: #[
    ]
arguments: #[
    value: [
            :string
             :literal
        ]
    ]
related: [
        "Numbers/csech"
         "Numbers/csec"
         "Numbers/cosh"
         "Numbers/acsech"
    ]