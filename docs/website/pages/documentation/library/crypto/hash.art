name: "hash"
type: :function
module: "Crypto"
description: "get hash for given value"
returns: [
        :integer
         :string
    ]
examples: [
        {
            print hash "hello"      ; 613153351
            print hash [1 2 3]      ; 645676735036410
            print hash 123          ; 123
            
            a: [1 2 3]
            b: [1 2 3]
            print (hash a)=(hash b) ; true
        }
    ]
