name: "suffix"
type: :function
module: "Strings"
description: "add given suffix to string"
returns: [
        :string
         :nothing
    ]
examples: [
        {
            suffix "hell" "o"                  ; => "hello"
        }
         {
            str: "hell"
            suffix 'str                        ; str: "hello"
        }
    ]
attributes: #[
    ]
arguments: #[
    string: [
            :string
             :literal
        ]
    suffix: [
            :string
        ]
    ]
related: [
        "Strings/prefix"
         "Strings/escape"
         "Strings/join"
         "Strings/strip"
    ]