name: "define"
type: :function
module: "Converters"
returns: [
        :nothing
    ]
description: "define new type with given characteristics"
example: {
        define :person [name surname][
            sayHello: function [][
                print ["Hello" this\name]
            ]
        ]
        
        do [
            a: to :person ["John" "Doe"]
            print a
            ; [name:John surname:Doe]
        
            print ["Your surname is:" a\surname]
            ; Doe
        
            sayHello a
            ; Hello John
        ]
    }
