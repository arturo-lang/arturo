topic: $[topic :string] -> print ~"\n>> |topic|"
passed: $[] -> print "[+] passed!"

topic Â« difference
do [
    set: [1 2 3 4] 
    set2: [3 4 5 6]

    a: new set
    b: #[ b: new set ]

    difference 'a set2
    difference 'b\b set2
    ensure -> a = b\b
    passed

]

