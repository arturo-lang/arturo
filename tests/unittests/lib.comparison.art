        -> greater? {/a/} #[a: "a"]                       -> greater? {/#000000/} #black                  
        -> greater? {/000000/} #black                     -> greater? {/var/} var 'regex?
        -> greater? {/regex?/} var 'regex?
    ] => not? passed
    
    ; for :binary
    ensure -> every? @[
        -> greater? to :binary 1 1                    -> greater? to :binary 1 1.0                
