do [

    a: [1 1 2 3 4 2 3 4 4 5 5 6 7], squeeze 'a
    ensure -> [1 2 3 4 2 3 4 5 6 7] =  a
    passed

    a: [1 [4 2 3] 1 2 3 [4 2 3] 4 4 5 5 [6 7] [6 7]]
    squeeze 'a
    ensure -> [1 [4 2 3] 1 2 3 [4 2 3] 4 5 [6 7]] = a
    passed
    
    a: "hellllo wooooorrrrld", squeeze 'a
    ensure -> "helo world" =  a
    passed

]

