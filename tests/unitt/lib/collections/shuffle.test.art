import {unitt}!
import relative {../../commons/functions}!

suite "shuffle" [

    test "preserves size and element types" [
        a: shuffle [1 2 3 4 5 6]
        assert -> 6 = size a
        assert -> every? a 'item -> integer? item
    ]

    test "works with strings" [
        a: shuffle ["Manchester" "Granada" "Boston" "Davos"]
        assert -> 4 = size a
        assert -> every? a 'item -> string? item
    ]

]
