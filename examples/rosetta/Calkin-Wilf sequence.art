n: 1
d: 1
calkinWilf: function [] .export:[n,d] [
    n: (d - n) + 2 * (n/d) * d 
    tmp: d
    d: n
    n: tmp
    return @[n d]
]

first20: [[1 1]] ++ map 1..19 => calkinWilf
print "The first 20 terms of the Calkwin-Wilf sequence are:"
print map first20 'f -> ~"|f\0|/|f\1|"

