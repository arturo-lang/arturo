prettyPrint: function [inp][
    loop.with:'i split.lines inp 'line [
        prints [pad to :string i*50 3 ":"]
        print split.every:10 line
    ]
    count: tally inp
    print ["Total count => A:" count\A, "T:" count\T "G:" count\G "C:" count\C]
]

