ourGamma: function [x][
    y: x - 1
    result: last A 
    loop ((size A)-1)..0 'n ->
        result: (result*y) + get A n
    result: 1 // result
    return result
]

loop 1..10 'z [
    v1: ourGamma z // 3
    v2: gamma z // 3
    print [
        pad (to :string z)++" =>" 10
        pad (to :string v1)++" ~" 30 
        pad (to :string v2)++" :" 30
        pad (to :string v1-v2) 30
    ]
]