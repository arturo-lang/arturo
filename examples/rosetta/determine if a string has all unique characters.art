    "", ".", "abcABC", "XYZ ZYX",
    "1234567890ABCDEFGHIJKLMN0PQRSTUVWXYZ",
    "01234567890ABCDEFGHIJKLMN0PQRSTUVWXYZ0X",
    "hÃ©tÃ©rogÃ©nÃ©itÃ©",
    "ğŸ†ğŸƒğŸ‡ğŸˆ", "ğŸ˜ğŸ˜€ğŸ™ŒğŸ’ƒğŸ˜ğŸ™Œ", "ğŸ ğŸŸğŸ¡ğŸ¦ˆğŸ¬ğŸ³ğŸ‹ğŸ¡"
]

loop strings 'str [
    chars: split str
    prints ["\"" ++ str ++ "\"" ~"(size |size str|):"]
    
    if chars = unique chars [
        print "has no duplicates."
        continue
    ]

    seen: #[]
    done: false

    i: 0
    while [and? i<size chars
                not? done][
        ch: chars\[i]
        switch not? key? seen ch [
            seen\[ch]: i
        ][
            print ~"has duplicate char `|ch|` on |get seen ch| and |i|"
            done: true
        ]
        i: i+1
    ]
]