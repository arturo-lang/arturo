FileExtensions #("zip" "rar" "7z" "gz" "archive" "A##" "tar.bz2")

hasExtension [file]{
	return $(contains FileExtensions $(replace $(getExt file) "." ""))
}

files #("MyData.a##" "MyData.tar.Gz" "MyData.gzip" "MyData.7z.backup" "MyData..." "MyData" "MyData_v1.0.tar.bz2" "MyData_v1.0.bz2")

loop files {
	print & + " => hasExtension: " + $(hasExtension &)
}

