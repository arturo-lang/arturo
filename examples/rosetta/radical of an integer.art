radical: $[n]-> product unique factors.prime n

(1..50) | map => radical
        | split.every:10
        | map => [join map & 'r -> pad to :string r 4]
        | print.lines

print ""
loop [99999, 499999, 999999] 'r ->
    print ["Radical of" r "->" radical r]
print ""

(1..1000000) | map => [size unique factors.prime &]
             | tally
             | loop [k,v] -> print [k ":" v]