getPrimes: function [upto][
    result: new [2]
    loop range.step:2 3 upto 'x [
        divisible: false
        loop 2..inc x/2 'z [
            if zero? x%z ->
                divisible:  true
        ]
        if not? divisible ->
            'result ++ x
    ]
    return result
]

print getPrimes 100