cmax: function => [
    m: neg âˆ
    map & 'x -> m:<=max @[m x]
]

vmin: $ => [map couple & & => min]

vsub: $ => [map couple & & 'p -> p\0 - p\1]

water: function [a][
    sum vsub vmin reverse cmax reverse a cmax a a
]

loop [
    [1, 5, 3, 7, 2],
    [5, 3, 7, 2, 6, 4, 5, 9, 1, 2],
    [2, 6, 3, 5, 2, 8, 1, 4, 2, 2, 5, 3, 5, 7, 4, 1],
    [5, 5, 5, 5],
    [5, 6, 7, 8],
    [8, 7, 7, 6],
    [6, 7, 10, 7, 6]
] 'a -> print [a "->" water a]